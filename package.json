{
    "name": "urltoimage",
    "displayName": "UrlToImage",
    "description": "Inlines the image at the URL selected.",
    "version": "0.0.1",
    "publisher": "xander",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.urlToImage"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "extension.urlToImage",
            "title": "URL To Image"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}